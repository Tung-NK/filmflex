{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "main": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/main",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "./public/asset_user/css/bootstrap.min.css",
              "./public/asset_user/css/splide.min.css",
              "./public/asset_user/css/slimselect.css",
              "./public/asset_user/css/plyr.css",
              "./public/asset_user/css/photoswipe.css",
              "./public/asset_user/css/default-skin.css",
              "./public/asset_user/css/main.css",
              "src/styles.css"
            ],
            "scripts": [
              "./public/asset_user/js/bootstrap.bundle.min.js",
              "./public/asset_user/js/splide.min.js",
              "./public/asset_user/js/slimselect.min.js",
              "./public/asset_user/js/smooth-scrollbar.js",
              "./public/asset_user/js/plyr.min.js",
              "./public/asset_user/js/photoswipe.min.js",
              "./public/asset_user/js/photoswipe-ui-default.min.js",
              "./public/asset_user/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "main:build:production"
            },
            "development": {
              "buildTarget": "main:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}